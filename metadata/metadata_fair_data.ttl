#################################################################
# FAIR DATA POINT
#################################################################
:WellsDataPoint a fdp:FAIRDataPoint ;
  fdp:startDate "2025-10-23T00:00:00"^^xsd:dateTime ;
  fdp:uiLanguage <http://id.loc.gov/vocabulary/iso639-1/en> ;
  fdp:metadataIdentifier <https://raw.githubusercontent.com/patriciaferreiradasilva/FAIR_petrokgraph/refs/heads/main/metadata/metadata_fair_data.ttl> ;

#################################################################
# CATALOG
#################################################################
  :WellsCatalog
  [ a dcat:Catalog ;
        dct:issued "2025-10-23T00:00:00"^^xsd:dateTime ;
  ];

#################################################################
# DATASET
#################################################################
  :WellsDataset
  [ a dcat:Dataset ;
        dct:issued "2025-10-21T08:23:00-03:00"^^xsd:dateTime ;
        dct:modified "2025-10-23T00:00:00"^^xsd:dateTime ;
        dcterms:title "FAIR Petro KGraph."^^xsd:string ;
        dcterms:description "FAIR Resource for wells from ANP inventory, with geographic and geological information."^^xsd:string ;
        dcterms:creator <https://https://github.com/patriciaferreiradasilva> ;
        dcterms:publisher 
        [ a foaf:Agent;
          foaf:name "Group 19"
        ];
        prov:wasDerivedFrom <https://https://github.com/patriciaferreiradasilva/FAIR_petrokgraph/tree/main/original%20data/Tabela_pocos_2025_Setembro_19.csv> ;
        dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
        dcat:theme <https://raw.githubusercontent.com/patriciaferreiradasilva/FAIR_petrokgraph/refs/heads/main/ontology/petrokgraph.owx#> ;
  ]; 

#################################################################
# DISTRIBUTION (CSV)
#################################################################

    :WellsDistribution
    [ a dcat:Distribution ;
        dcat:accessURL <https://raw.githubusercontent.com/patriciaferreiradasilva/FAIR_petrokgraph/refs/heads/main/data/fair_data.ttl> ;
        dcat:format "CSV" ;
        dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
        dcat:mediaType "turtle" ;
    ]
.